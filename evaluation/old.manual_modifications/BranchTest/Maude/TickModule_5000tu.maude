mod TICKRULE is
  pr BRANCHTEST .
  pr @PCMFLATTEN@BEHAVIOR@ .
  var T TE : Time .
  var MODEL : @Model .

  crl [tick] :
     { MODEL } in time T
  =>
    { delta(MODEL, TE) } in time (T plus TE)
  if TE := mte(MODEL) /\ TE =/= zero 
		/\ (T plus TE) le toRat(5000) .
endm

rewrite init(BRANCHTEST) .

q
