mod TICKRULE is
  pr EXTERNALCALLS .
  pr @PCMFLATTEN@BEHAVIOR@ .
  var T TE : Time .
  var MODEL : @Model .

  crl [tick] :
     { MODEL } in time T
  =>
    { delta(MODEL, TE) } in time (T plus TE)
  if TE := mte(MODEL) /\ TE =/= zero /\ (T plus TE) le rat(10.00)
  [print "Applied tick rule. TE := " TE] .
endm

rewrite init(EXTERNALCALLS) .

q
